case 2:
pop();
break; case 3:
display();
break;
case 4:
break; default;
printf(“\n\t invalid choice:“);
) while (ch!==4);
Void push(int n)
int item;
if (top==n-1)
Printf(“\n stack is overflow: insertion not pos5ible”);
else
printf(“\n Enter the element to be inserted:”);
Scanf(“%d”,&item); top=top+1; S[top]=item;
return;
Void pop()
int item; if(top==-1)
Printf(“stack empty: deletion not possible”);
else
item=5|top];
top=top-1;
printf(“deleted item is %d”,item);
void di5play()
int i; if(top==-1)
Printf(“stack empty”);
else
i=top;
while(i>=0)
Printf(“%d“,[i]); i=i-1;
return;
